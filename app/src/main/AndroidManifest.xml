            android:theme="@style/Theme.Pitchify_Main"/>

        <activity
            android:name=".resources_screens.learning_resources_quiz_start"
            android:exported="true"
            android:theme="@style/Theme.Pitchify_Main"/>

        <activity
            android:name=".resources_screens.learning_resources_quiz_one"
            android:exported="true"
            android:theme="@style/Theme.Pitchify_Main"/>

        <activity
            android:name=".resources_screens.learning_resources_quiz_two"
            android:exported="true"
            android:theme="@style/Theme.Pitchify_Main"/>

        <activity
            android:name=".resources_screens.learning_resources_quiz_four"
            android:exported="true"
            android:theme="@style/Theme.Pitchify_Main">


        <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>



    </application>

</manifest>