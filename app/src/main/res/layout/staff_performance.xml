        app:layout_constraintVertical_bias="0.623" />

    <ImageView
        android:id="@+id/dashboard_badge1"
        android:layout_width="43dp"
        android:layout_height="45dp"
        android:scaleType="fitXY"
        android:src="@drawable/dashboard_badge6"
        app:layout_constraintBottom_toBottomOf="@+id/dashboard_imageView2"
        app:layout_constraintEnd_toEndOf="@+id/dashboard_imageView2"
        app:layout_constraintHorizontal_bias="0.137"
        app:layout_constraintStart_toStartOf="@+id/dashboard_imageView2"
        app:layout_constraintTop_toTopOf="@+id/dashboard_imageView2"
        app:layout_constraintVertical_bias="0.39"
