        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/dashboard_imageView4"
        app:layout_constraintEnd_toEndOf="@+id/dashboard_imageView4"
        app:layout_constraintHorizontal_bias="0.177"
        app:layout_constraintStart_toStartOf="@+id/dashboard_imageView4"
        app:layout_constraintTop_toTopOf="@+id/dashboard_imageView4"
        app:layout_constraintVertical_bias="0.147" />

    <TextView
        android:id="@+id/dashboard_interactivity_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="55%"
        android:textColor="#000000"
        android:textSize="52sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/dashboard_imageView4"
        app:layout_constraintEnd_toEndOf="@+id/dashboard_imageView4"
        app:layout_constraintHorizontal_bias="0.235"
        app:layout_constraintStart_toStartOf="@+id/dashboard_imageView4"
        app:layout_constraintTop_toTopOf="@+id/dashboard_imageView4"
        app:layout_constraintVertical_bias="0.611" />

    <ImageView
        android:id="@+id/dashboard_imageView5"
        android:layout_width="wrap_content"
