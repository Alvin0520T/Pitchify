<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E1FBFF"
    tools:layout_editor_absoluteX="1dp"
    tools:layout_editor_absoluteY="32dp">

    <ImageButton
        android:id="@+id/backbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="#00000000"
        android:src="@drawable/aipitching_left_icon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/staff_performance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Staff Performance"
        android:textColor="#101828"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022" />


    <TextView
        android:id="@+id/userNameTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Alvin Tang"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dashboard_profile" />

    <ImageView
        android:id="@+id/dashboard_profile"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="14dp"
        android:scaleType="fitXY"
        android:src="@drawable/profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/staff_performance"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/dashboard_imageView1"
        android:layout_width="354dp"
        android:layout_height="136dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/content_dashboard"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.456"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userNameTextView"
        app:layout_constraintVertical_bias="0.07"></ImageView>

    <TextView
        android:id="@+id/dashboard_pitch_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="AI Simulation Pitching Score"
        android:textColor="#343434"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/dashboard_imageView1"
        app:layout_constraintEnd_toEndOf="@+id/dashboard_imageView1"
        app:layout_constraintHorizontal_bias="0.105"
        app:layout_constraintStart_toStartOf="@+id/dashboard_imageView1"
        app:layout_constraintTop_toTopOf="@+id/dashboard_imageView1"
        app:layout_constraintVertical_bias="0.119" />

    <TextView
        android:id="@+id/dashboard_percentage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="63%"
        android:textColor="#000000"
        android:textSize="46sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/dashboard_imageView1"
        app:layout_constraintEnd_toEndOf="@+id/dashboard_imageView1"
        app:layout_constraintHorizontal_bias="0.063"
        app:layout_constraintStart_toStartOf="@+id/dashboard_imageView1"
        app:layout_constraintTop_toTopOf="@+id/dashboard_imageView1"
        app:layout_constraintVertical_bias="0.426" />


    <ImageView
        android:id="@+id/dashboard_imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/content_dashboard2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.106"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.555"></ImageView>

    <TextView
        android:id="@+id/dashboard_badge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Badges"
        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/dashboard_imageView2"
        app:layout_constraintEnd_toEndOf="@+id/dashboard_imageView2"
        app:layout_constraintHorizontal_bias="0.142"
        app:layout_constraintStart_toStartOf="@+id/dashboard_imageView2"
        app:layout_constraintTop_toTopOf="@+id/dashboard_imageView2"
        app:layout_constraintVertical_bias="0.102" />


    <ImageView
        android:id="@+id/dashboard_imageView3"
        android:layout_width="wrap_content"
