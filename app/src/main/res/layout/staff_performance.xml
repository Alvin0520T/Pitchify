        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.877"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.555"></ImageView>

    <TextView
        android:id="@+id/dashboard_talk_ratio_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="46%"
        android:textColor="#000000"
        android:textSize="52sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/dashboard_imageView3"
        app:layout_constraintEnd_toEndOf="@+id/dashboard_imageView3"
        app:layout_constraintHorizontal_bias="0.264"
        app:layout_constraintStart_toStartOf="@+id/dashboard_imageView3"
        app:layout_constraintTop_toTopOf="@+id/dashboard_imageView3"
        app:layout_constraintVertical_bias="0.623" />

    <TextView
        android:id="@+id/dashboard_talk_ratio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/talk_ratio"
        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/dashboard_imageView3"
        app:layout_constraintEnd_toEndOf="@+id/dashboard_imageView3"
        app:layout_constraintHorizontal_bias="0.168"
        app:layout_constraintStart_toStartOf="@+id/dashboard_imageView3"
        app:layout_constraintTop_toTopOf="@+id/dashboard_imageView3"
        app:layout_constraintVertical_bias="0.102" />

