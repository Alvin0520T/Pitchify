<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    >

    <ImageButton
        android:id="@+id/aipitching_performanceanalysis_backbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="#00000000"
        android:src="@drawable/aipitching_left_icon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/aipitching_performanceanalysis_text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Performance"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.372"
        app:layout_constraintStart_toEndOf="@+id/aipitching_performanceanalysis_backbutton"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/aipitching_performanceanalysis_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="Transcription"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.047"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/aipitching_performanceanalysis_imageView1"
        android:layout_width="379dp"
        android:layout_height="80dp"
        android:layout_marginTop="17dp"
        android:background="@drawable/aipitching_performanceanalysis_greybox"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/aipitching_performanceanalysis_text2" />

    <TextView
        android:id="@+id/aipitching_performanceanalysis_text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:text="20:50"
        app:layout_constraintEnd_toEndOf="@+id/aipitching_performanceanalysis_imageView1"
        app:layout_constraintHorizontal_bias="0.033"
        app:layout_constraintStart_toStartOf="@+id/aipitching_performanceanalysis_imageView1"
        app:layout_constraintTop_toTopOf="@+id/aipitching_performanceanalysis_imageView1" />

    <TextView
        android:id="@+id/aipitching_performanceanalysis_text4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Hello! I'm Alex. What are you looking for in a laptop?"
        app:layout_constraintEnd_toEndOf="@+id/aipitching_performanceanalysis_imageView1"
        app:layout_constraintHorizontal_bias="0.214"
        app:layout_constraintStart_toStartOf="@+id/aipitching_performanceanalysis_imageView1"
        app:layout_constraintTop_toBottomOf="@+id/aipitching_performanceanalysis_text3" />

    <ImageView
        android:id="@+id/aipitching_performanceanalysis_imageView2"
        android:layout_width="379dp"
        android:layout_height="92dp"
        android:layout_marginTop="6dp"
        android:background="@drawable/aipitching_performanceanalysis_bluebox"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.468"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/aipitching_performanceanalysis_imageView1" />

    <TextView
        android:id="@+id/aipitching_performanceanalysis_text5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:text="20:51"
        app:layout_constraintEnd_toEndOf="@+id/aipitching_performanceanalysis_imageView1"
        app:layout_constraintHorizontal_bias="0.033"
        app:layout_constraintStart_toStartOf="@+id/aipitching_performanceanalysis_imageView1"
        app:layout_constraintTop_toTopOf="@+id/aipitching_performanceanalysis_imageView2" />

    <TextView
        android:id="@+id/aipitching_performanceanalysis_text6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="I need a fast laptop for graphic design, but I'm \nconcerned about the price."
        app:layout_constraintEnd_toEndOf="@+id/aipitching_performanceanalysis_imageView1"
        app:layout_constraintHorizontal_bias="0.142"
        app:layout_constraintStart_toStartOf="@+id/aipitching_performanceanalysis_imageView1"
        app:layout_constraintTop_toBottomOf="@+id/aipitching_performanceanalysis_text5" />

    <ImageView
        android:id="@+id/aipitching_performanceanalysis_imageView3"
        android:layout_width="379dp"
        android:layout_height="132dp"
        android:layout_marginTop="6dp"
        android:background="@drawable/aipitching_performanceanalysis_greybox"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/aipitching_performanceanalysis_imageView2" />

    <TextView
        android:id="@+id/aipitching_performanceanalysis_text7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:text="20:51"
        app:layout_constraintEnd_toEndOf="@+id/aipitching_performanceanalysis_imageView3"
        app:layout_constraintHorizontal_bias="0.033"
        app:layout_constraintStart_toStartOf="@+id/aipitching_performanceanalysis_imageView3"
        app:layout_constraintTop_toTopOf="@+id/aipitching_performanceanalysis_imageView3" />

    <TextView
        android:id="@+id/aipitching_performanceanalysis_text8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:text="20:51"
        app:layout_constraintEnd_toEndOf="@+id/aipitching_performanceanalysis_imageView3"
        app:layout_constraintHorizontal_bias="0.033"
        app:layout_constraintStart_toStartOf="@+id/aipitching_performanceanalysis_imageView3"
        app:layout_constraintTop_toTopOf="@+id/aipitching_performanceanalysis_imageView3" />

    <TextView
        android:id="@+id/aipitching_performanceanalysis_text9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="No worries. Our ProDesign series is perfect for \ngraphic design, ensuring smooth performance. It's \nan investment in your productivity, and we currently \noffer a promotion with a free extended warranty and \ndesign software bundle."
        app:layout_constraintEnd_toEndOf="@+id/aipitching_performanceanalysis_imageView3"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="@+id/aipitching_performanceanalysis_imageView3"
        app:layout_constraintTop_toBottomOf="@+id/aipitching_performanceanalysis_text7" />

    <ImageView
        android:id="@+id/aipitching_performanceanalysis_imageView4"
        android:layout_width="379dp"
        android:layout_height="71dp"
        android:layout_marginTop="6dp"
        android:background="@drawable/aipitching_performanceanalysis_bluebox"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/aipitching_performanceanalysis_imageView3" />

    <TextView
        android:id="@+id/aipitching_performanceanalysis_text10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:text="20:51"
        app:layout_constraintEnd_toEndOf="@+id/aipitching_performanceanalysis_imageView4"
        app:layout_constraintHorizontal_bias="0.033"
        app:layout_constraintStart_toStartOf="@+id/aipitching_performanceanalysis_imageView4"
        app:layout_constraintTop_toTopOf="@+id/aipitching_performanceanalysis_imageView4" />

    <TextView
        android:id="@+id/aipitching_performanceanalysis_text11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Sounds good, let's proceed."
        app:layout_constraintEnd_toEndOf="@+id/aipitching_performanceanalysis_imageView4"
        app:layout_constraintHorizontal_bias="0.054"
        app:layout_constraintStart_toStartOf="@+id/aipitching_performanceanalysis_imageView4"
        app:layout_constraintTop_toBottomOf="@+id/aipitching_performanceanalysis_text10" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="379dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:background="@drawable/aipitching_performanceanalysis_line"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/aipitching_performanceanalysis_imageView4" />

    <TextView
        android:id="@+id/aipitching_performanceanalysis_text12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Voice Recorded"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="8dp"
        android:background="@drawable/aipitching_performanceanalysis_voicerecordedbutton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.033"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/aipitching_performanceanalysis_text12" />

    <TextView
        android:id="@+id/aipitching_performanceanalysis_text13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="18dp"
        android:text="3m 24s"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/aipitching_performanceanalysis_text12" />

    <Button
        android:id="@+id/aipitching_performanceanalysis_scorebutton"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:backgroundTint="#D8FE83"
        android:text="Your score: 89%"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:layout_marginBottom="30dp"
        app:layout_constraintBottom_toTopOf="@+id/bottomNavigationView"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/aipitching_performanceanalysis_feedbackbutton"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:backgroundTint="#FEE164"
        android:text="View feedback"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:layout_marginBottom="30dp"
        app:layout_constraintBottom_toTopOf="@+id/bottomNavigationView"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/botnavigation_bar" />

</androidx.constraintlayout.widget.ConstraintLayout>