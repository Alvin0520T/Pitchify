        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="63%"
        android:textColor="#000000"
        android:textSize="46sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/dashboard_imageView1"
        app:layout_constraintEnd_toEndOf="@+id/dashboard_imageView1"
        app:layout_constraintHorizontal_bias="0.064"
        app:layout_constraintStart_toStartOf="@+id/dashboard_imageView1"
        app:layout_constraintTop_toTopOf="@+id/dashboard_imageView1"
        app:layout_constraintVertical_bias="0.417" />


    <ImageView
        android:id="@+id/dashboard_imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/content_dashboard2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.102"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
    </ImageView>

    <TextView
        android:id="@+id/dashboard_badge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Badges"
        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/dashboard_imageView2"
