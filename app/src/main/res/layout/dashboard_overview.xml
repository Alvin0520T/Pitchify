    <ImageView
        android:id="@+id/dashboard_badge4"
        android:layout_width="43dp"
        android:layout_height="45dp"
        android:scaleType="fitXY"
        android:src="@drawable/dashboard_badge3"
        app:layout_constraintBottom_toBottomOf="@+id/dashboard_imageView2"
        app:layout_constraintEnd_toEndOf="@+id/dashboard_imageView2"
        app:layout_constraintHorizontal_bias="0.911"
