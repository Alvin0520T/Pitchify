<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/light_blue"
    tools:ignore="ExtraText">

    <ImageView
        android:id="@+id/admin_register_create_account"
        android:layout_width="188dp"
        android:layout_height="180dp"
        android:src="@drawable/a_register_tick"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.45"
        tools:layout_editor_absoluteX="82dp" />

    <ImageView
        android:id="@+id/admin_register_back_to_admin_login_button"
        android:layout_width="267dp"
        android:layout_height="111dp"
        android:src="@drawable/a_register_back_admin"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/admin_register_success"
        app:layout_constraintVertical_bias="0.623"
        tools:layout_editor_absoluteX="82dp" />

    <ImageView
        android:id="@+id/admin_register_back_to_user_login_button"
        android:layout_width="265dp"
        android:layout_height="116dp"
        android:src="@drawable/a_register_back_user"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/admin_register_success"
        app:layout_constraintVertical_bias="0.749"
        tools:layout_editor_absoluteX="82dp" />

    <ImageView
        android:id="@+id/admin_register_create_another_account"
        android:layout_width="221dp"
        android:layout_height="88dp"
        android:src="@drawable/a_register_create_another"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/admin_register_success"
        app:layout_constraintVertical_bias="0.829" />

    <ImageView
        android:id="@+id/admin_register_success"
        android:layout_width="293dp"
        android:layout_height="101dp"
        android:src="@drawable/a_register_success"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.203"
        tools:layout_editor_absoluteX="82dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
