<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/light_blue"
    tools:ignore="ExtraText">

    <ImageView
        android:id="@+id/admin_register_create_account"
        android:layout_width="188dp"
        android:layout_height="180dp"
        android:layout_marginTop="28dp"
        android:src="@drawable/a_register_tick"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/admin_register_success" />

    <ImageView
        android:id="@+id/admin_register_back_to_admin_login_button"
        android:layout_width="267dp"
        android:layout_height="111dp"
        android:src="@drawable/a_register_back_admin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/admin_register_create_account" />

    <ImageView
        android:id="@+id/admin_register_back_to_user_login_button"
        android:layout_width="270dp"
        android:layout_height="102dp"
        android:src="@drawable/a_register_back_user"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/admin_register_back_to_admin_login_button" />

    <ImageView
        android:id="@+id/admin_register_create_another_account"
        android:layout_width="221dp"
        android:layout_height="88dp"
        android:src="@drawable/a_register_create_another"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/admin_register_back_to_user_login_button"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/admin_register_success"
        android:layout_width="293dp"
        android:layout_height="101dp"
        android:layout_marginTop="28dp"
        android:src="@drawable/a_register_success"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
